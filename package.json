{
  "name": "@felsgorilla/angular6-toaster",
  "version": "1.0.0",
  "description": "An Angular Toaster Notification library based on AngularJS-Toaster",
  "main": "dist/bundles/angular5-toaster.umd.js",
  "module": "angular6-toaster.js",
  "typings": "angular6-toaster.d.ts",
  "scripts": {
    "start": "npm run test && http-server -c-1 -o -p 8875 .",
    "prebuild": "rimraf ./dist && npm run lint",
    "build": "node_modules/.bin/ngc -p ./tsconfig-aot.json && npm run rollup",
    "rollup": "node_modules/.bin/rollup -c ./config/rollup.config.js && npm run uglify",
    "uglify": "node_modules/.bin/uglifyjs ./dist/bundles/angular6-toaster.umd.js -o ./dist/bundles/angular6-toaster.umd.min.js && npm run copy",
    "copy": "cpx package.json dist && cpx LICENSE dist && cpx README.md dist && cpx ./src/toaster.scss dist",
    "lint": "node_modules/.bin/tslint -c tslint.json 'src/**/*.ts'",
    "test": "karma start karma.conf.js"
  },
  "keywords": [
    "angular",
    "angular6",
    "angular-toaster",
    "angular6-toaster",
    "toast",
    "toaster",
    "notification"
  ],
  "author": "Paul Keller",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/FelsGorilla/angular6-toaster"
  },
  "bugs": {
    "url": "https://github.com/FelsGorilla/angular6-toaster/issues"
  },
  "peerDependencies": {
    "@angular/common": ">=6.0.0 ",
    "@angular/compiler": ">=6.0.0",
    "@angular/core": ">=6.0.0",
    "rxjs": ">=6.0.0"
  },
  "devDependencies": {
    "@angular/animations": "6.1.6",
    "@angular/common": "6.1.6",
    "@angular/compiler": "6.1.6",
    "@angular/compiler-cli": "6.1.6",
    "@angular/core": "6.1.6",
    "@angular/platform-browser": "6.1.6",
    "@angular/platform-browser-dynamic": "6.1.6",
    "@angular/platform-server": "6.1.6",
    "@types/jasmine": "2.5.47",
    "@types/node": "7.0.12",
    "awesome-typescript-loader": "3.1.2",
    "codelyzer": "^4.4.4",
    "core-js": "2.4.1",
    "coveralls": "2.11.14",
    "cpx": "1.5.0",
    "http-server": "0.9.0",
    "istanbul-instrumenter-loader": "1.1.0",
    "jasmine-core": "2.5.2",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.0.2",
    "karma-mocha-reporter": "^2.1.0",
    "karma-remap-coverage": "~0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.3",
    "reflect-metadata": "0.1.10",
    "remap-istanbul": "0.8.3",
    "rimraf": "2.5.4",
    "rollup": "0.41.6",
    "rxjs": "6.3.3",
    "source-map-loader": "0.2.0",
    "ts-helpers": "1.1.2",
    "tslint": "^5.11.0",
    "typescript": "^2.9.2",
    "typings": "2.1.0",
    "uglify-js": "2.8.18",
    "webpack": "2.3.2",
    "zone.js": "^0.8.26"
  }
}
